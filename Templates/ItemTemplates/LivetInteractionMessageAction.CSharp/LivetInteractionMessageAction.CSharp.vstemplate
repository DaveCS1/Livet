<?xml version="1.0" encoding="utf-8"?>

<VSTemplate Version="3.0.0" Type="Item" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://schemas.microsoft.com/developer/vstemplate/2005">
  <TemplateData>
    <Name>Livet WPF interaction action</Name>
    <Description>Create an interaction action for Livet</Description>
    <ProjectType>CSharp</ProjectType>
    <ProjectSubType>Windows</ProjectSubType>
    <SortOrder>0</SortOrder>
    <RequiredFrameworkVersion>4.5.2</RequiredFrameworkVersion>
    <DefaultName>Action.cs</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <Icon>LivetAction.ico</Icon>
